<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>用户详情页面</title>
<style>
	*{
		margin:0px;
		padding:0px;
		font-family: "microsoft yahei";
	}
	li{
		list-style-type:none;
		text-decoration: none;
	}
	.info-left{
		width:398px;
		height: 100%;
		background: pink;
		float:left;
	}
	.attrname{
		width:86px;
		height: 36px;
		display: block;
		float: left;
		text-align: center;
		line-height: 36px;
		letter-spacing: 6px;
		font-size: 16px;
	}
	.attrvalue{
		width:266px;
		height: 36px;
		background: gray;
		display: block;
		float: left;
	}
</style>
</head>
<body>
	<div style="width:766px;height: 566px;background: gray;margin: 0 auto;">
		<div class="info-left">
			<ul>
				<li><span class="attrname">用户id:</span><span class="attrvalue"></span></li>
			</ul>
			
		</div>
		
	</div>
</body>
</html>
